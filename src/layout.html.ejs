<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <title><%= title %></title>
        <% stylesheets.forEach(stylesheet => { %>
        <link rel="stylesheet" href="<%= stylesheet %>" />
        <% }) %>
    </head>
    <body>
        <%- content %> <% javascripts.forEach(javascript => { %>
        <script src="<%= javascript %>"></script>
        <% }) %>
    </body>
</html>
